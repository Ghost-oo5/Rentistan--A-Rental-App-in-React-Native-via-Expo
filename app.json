{
  "expo": {
    "name": "FYP-practice",
    "slug": "FYP-practice",
    "version": "1.0.0",
    "scheme": "rentistan",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.abdulbasit.FYPpractice", 
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"] 
      }
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Rentistan-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdulbasit.FYPpractice"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a899cf2-c0d7-490d-a7b7-ef78a70488fb"
      }
    },
    "plugins": [
      "expo-font",
      "expo-notifications" 
    ]
  }
}
